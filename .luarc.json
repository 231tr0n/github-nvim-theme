{
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "lua/?.lua",
      "lua/?/init.lua"
    ],
    "pathStrict": true
  },
  "workspace": {
    "checkThirdParty": "Disable",
    "library": [
      "test/plenary"
    ],
    "ignoreDir": [
      "/.meta",
      "/.log",
      ".vscode"
    ]
  },
  "diagnostics": {
    "unusedLocalExclude": [
      "_*"
    ]
  }
}
